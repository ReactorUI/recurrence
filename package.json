{
  "name": "@reactorui/recurrence",
  "version": "1.0.10",
  "description": "A powerful, flexible recurrence rule builder for React applications",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsup",
    "build:watch": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.{js,jsx,ts,tsx}"
    ],
    "moduleNameMapping": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts"
    ]
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "lucide-react": "^0.539.0",
    "clsx": "^2.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vite": "^4.4.0",
    "@vitejs/plugin-react": "^4.0.0",
    "tailwindcss": "^3.3.0",
    "postcss": "^8.4.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.45.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "prettier": "^3.0.0",
    "eslint-config-prettier": "^9.0.0",
    "tsup": "^7.2.0"
  },
  "keywords": [
    "react",
    "recurrence",
    "rrule",
    "calendar",
    "scheduling",
    "date",
    "recurring",
    "ui",
    "component"
  ],
  "author": "ReactorUI",
  "license": "MIT",
  "homepage": "https://github.com/ReactorUI/recurrence#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ReactorUI/recurrence.git"
  },
  "bugs": {
    "url": "https://github.com/ReactorUI/recurrence/issues"
  }
}
